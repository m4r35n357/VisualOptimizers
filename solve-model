#!/bin/sh

GRY='\e[1;30m'
RED='\e[0;31m'
GRN='\e[0;32m'
MGT='\e[0;35m'
NRM='\e[0m'

args="$0 $*"
n_args=$(($# + 1))
echo "${GRY}args ${NRM}${n_args}${GRY} [ ${MGT}${args}${GRY} ]${NRM}" >&2

model=$1
order=$2
agents=$3
iterations=$4
runs=$5
lower=$6
upper=$7

# Multiple runs
./nm-$model-std 3 0 $order 1.0e-6 1000000 $(echo "scale=3; 0.5 * ($upper - $lower);" | /usr/bin/bc) 1 $runs $lower $upper >/dev/null
echo ""

# Periodic Descent Direction Setting
./spiral-$model-std 3 0 $order $agents $iterations 0 $lower $upper >/dev/null
echo ""

# Convergence Setting
./spiral-$model-std 3 0 $order $agents $iterations 1 $lower $upper >/dev/null
echo ""

# Un-clamped
./cut-$model-std 3 0 $order $agents $iterations 0 $lower $upper >/dev/null
echo ""

# Clamped
./cut-$model-std 3 0 $order $agents $iterations 1 $lower $upper >/dev/null
echo ""

# Base-line
./rnd-$model-std 3 0 $order $((agents * iterations)) 0 $lower $upper >/dev/null
echo ""
